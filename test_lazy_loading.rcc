// Test lazy loading of native modules
// This should not load all modules at startup - only when accessed

// Only accessing math module
let x = math.sqrt(16);
println("sqrt(16) = " + x);

// Only accessing string module
let s = "hello world";
let upper = string.upper(s);
println("upper(\"hello world\") = " + upper);

// Only accessing array module
let arr = [1, 2, 3, 4, 5];
let len = array.length(arr);
println("array.length([1, 2, 3, 4, 5]) = " + len);

println("All lazy-loaded modules work correctly!");
