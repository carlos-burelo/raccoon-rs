print("10. CLASS TYPES");
class Animal {
    public name: str;
    private age: int;

    constructor(name: str, age: int) {
        this.name = name;
        this.age = age;
    }

    public getName(): str {
        return this.name;
    }

    public getAge(): int {
        return this.age;
    }
}

class Dog extends Animal {
    public breed: str;

    constructor(name: str, age: int, breed: str) {
        super(name, age);
        this.breed = breed;
    }
}

let dog: Dog = new Dog("Buddy", 5, "Labrador");
print("✓ Classes with inheritance working\n");




print("11. INTERFACE TYPES");
interface Drawable {
    draw(): void;
    getColor(): str;
}

interface Resizable {
    resize(width: int, height: int): void;
    getSize(): [int, int];
}
