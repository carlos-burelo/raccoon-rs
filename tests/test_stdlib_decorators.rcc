// Test: Usando decoradores_demo stdlib

import { abs, max, min, clamp, fibonacci } from "std:decorators_demo";

// Test pure functions
let a = abs(-5.5);
let b = max(10.0, 20.0);
let c = min(10.0, 20.0);
let d = clamp(15.0, 10.0, 20.0);

print("abs(-5.5) = " + a);
print("max(10, 20) = " + b);
print("min(10, 20) = " + c);
print("clamp(15, 10, 20) = " + d);

// Test cached function
print("fibonacci(10) = " + fibonacci(10));
print("fibonacci(15) = " + fibonacci(15));
