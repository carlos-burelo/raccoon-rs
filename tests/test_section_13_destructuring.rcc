interface ICoord {
    x: int
    y: int
}
let { x: coordX, y: coordY }: ICoord = { x: 100, y: 200 }
print(`  ✓ Object destructuring: ${coordX}, ${coordY}`)


let { x, y }: ICoord = { x: 50, y: 75 }
print(`  ✓ Object destructuring shorthand: ${x}, ${y}`)


let [[g1, g2], [g3, g4]]: int[][] = [[1, 2], [3, 4]]
print(`  ✓ Nested destructuring: ${g1}, ${g2}, ${g3}, ${g4}`)


fn destructParam({ x, y }: ICoord): int {
    return x + y
}
let destructResult: int = destructParam({ x: 10, y: 20 })
print(`  ✓ Function param destructuring: ${destructResult}`)

