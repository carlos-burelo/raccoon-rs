


@deprecated("Use newFunction instead")
fn oldFunction(): int {
    return 42;
}

fn newFunction(): int {
    return 100;
}


@pure()
fn add(a: int, b: int): int {
    return a + b;
}


@pure()
@inline()
fn multiply(a: int, b: int): int {
    return a * b;
}


@cache(5000)
fn expensiveCalculation(n: int): int {
    let result = 0;
    for (let i = 0; i < n; i = i + 1) {
        result = result + i;
    }
    return result;
}


fn main(): void {
    
    let sum = add(5, 3);
    print("5 + 3 = " + sum);  

    let prod = multiply(4, 5);
    print("4 * 5 = " + prod);  

    
    let calc1 = expensiveCalculation(100);
    print("First call: " + calc1);

    
    let calc2 = expensiveCalculation(100);
    print("Second call: " + calc2);

    
    let oldResult = oldFunction();
    print("Old result: " + oldResult);
}

main();
