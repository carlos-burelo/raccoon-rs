// Test: Biblioteca Estándar - Módulo Math
// Demuestra cómo la stdlib implementada en Raccoon funciona sin primitivas nativas

import {
    abs, min, max, clamp, lerp, sign,
    floor, ceil, round,
    factorial, gcd, lcm,
    toRadians, toDegrees,
    isEven, isOdd, isPrime,
    PI, E
} from "std:math";

print("=== TEST: Math Module (Raccoon stdlib) ===\n");

// Test de funciones básicas
print("abs(-5.7):", abs(-5.7));           // 5.7
print("min(3, 1, 4, 1, 5):", min(3.0, 1.0, 4.0, 1.0, 5.0));  // 1.0
print("max(3, 1, 4, 1, 5):", max(3.0, 1.0, 4.0, 1.0, 5.0));  // 5.0
print("clamp(150, 0, 100):", clamp(150.0, 0.0, 100.0));       // 100.0
print("lerp(0, 100, 0.5):", lerp(0.0, 100.0, 0.5));          // 50.0
print("sign(-42):", sign(-42.0));                             // -1.0

print("\n--- Redondeo ---");
print("floor(3.7):", floor(3.7));         // 3.0
print("floor(-3.7):", floor(-3.7));       // -4.0
print("ceil(3.2):", ceil(3.2));           // 4.0
print("round(3.5):", round(3.5));         // 4.0
print("round(3.4):", round(3.4));         // 3.0

print("\n--- Matemáticas Enteras ---");
print("factorial(5):", factorial(5));     // 120
print("gcd(48, 18):", gcd(48, 18));       // 6
print("lcm(12, 18):", lcm(12, 18));       // 36

print("\n--- Conversión de Ángulos ---");
print("toRadians(180):", toRadians(180.0));  // ~PI
print("toDegrees(PI):", toDegrees(PI));      // 180.0

print("\n--- Predicados ---");
print("isEven(4):", isEven(4));           // true
print("isOdd(7):", isOdd(7));             // true
print("isPrime(17):", isPrime(17));       // true
print("isPrime(15):", isPrime(15));       // false

print("\n--- Constantes ---");
print("PI:", PI);                          // 3.14159...
print("E:", E);                            // 2.71828...

print("\n--- Prueba de Algoritmo Complejo ---");
print("Primeros 10 números primos:");
let count = 0;
let n = 2;
while (count < 10) {
    if (isPrime(n)) {
        print(n);
        count = count + 1;
    }
    n = n + 1;
}

print("\n✅ Todas las funciones implementadas en Raccoon puro!");
print("⚡ Sin acceso al runtime nativo - solo operadores de Raccoon");
