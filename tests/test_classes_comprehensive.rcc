print("‚úÖ Test 2: Class with Methods")

class Calculator {
    value: int
    
    constructor(initial: int) {
        this.value = initial
    }
    
    add(n: int): void {
        this.value = this.value + n
    }
    
    subtract(n: int): void {
        this.value = this.value - n
    }
    
    multiply(n: int): void {
        this.value = this.value * n
    }
    
    getValue(): int {
        return this.value
    }
    
    reset(): void {
        this.value = 0
    }
}

let calc: Calculator = new Calculator(10)
print(`  Initial value: ${calc.getValue()}`)
calc.add(5)
print(`  After add(5): ${calc.getValue()}`)
calc.multiply(2)
print(`  After multiply(2): ${calc.getValue()}`)
calc.subtract(10)
print(`  After subtract(10): ${calc.getValue()}`)
calc.reset()
print(`  After reset: ${calc.getValue()}`)





print("‚úÖ Test 3: Class with Different Data Types")

class Product {
    name: str
    price: float
    inStock: bool
    quantity: int
    
    constructor(name: str, price: float, quantity: int) {
        this.name = name
        this.price = price
        this.quantity = quantity
        this.inStock = true
    }
    
    getTotalValue(): float {
        return this.price * this.quantity
    }
    
    sell(amount: int): void {
        if (this.quantity >= amount) {
            this.quantity = this.quantity - amount
            if (this.quantity == 0) {
                this.inStock = false
            }
        }
    }
    
    getInfo(): str {
        return this.name + " - $" + this.price + " (Stock: " + this.quantity + ")"
    }
}

let product: Product = new Product("Laptop", 999.99, 5)
print(`  ${product.getInfo()}`)
print(`  Total value: $${product.getTotalValue()}`)
print(`  In stock: ${product.inStock}`)
product.sell(3)
print(`  After selling 3: ${product.getInfo()}`)
product.sell(2)
print(`  After selling 2 more: ${product.getInfo()}`)
print(`  In stock: ${product.inStock}`)





print("‚úÖ Test 4: Class with Array Properties")

class Classroom {
    name: str
    students: str[]
    grades: int[]
    
    constructor(name: str) {
        this.name = name
        this.students = []
        this.grades = []
    }
    
    addStudent(student: str, grade: int): void {
        this.students.push(student)
        this.grades.push(grade)
    }
    
    getStudentCount(): int {
        return this.students.length
    }
    
    getAverageGrade(): float {
        if (this.grades.length == 0) {
            return 0.0
        }
        let sum: int = 0
        for (const i in 0..this.grades.length - 1) {
            sum = sum + this.grades[i]
        }
        return sum / this.grades.length
    }
}

let classroom: Classroom = new Classroom("Math 101")
print(`  Classroom: ${classroom.name}`)
classroom.addStudent("Alice", 95)
classroom.addStudent("Bob", 87)
classroom.addStudent("Charlie", 92)
print(`  Students: ${classroom.getStudentCount()}`)
print(`  Average grade: ${classroom.getAverageGrade()}`)





print("‚úÖ Test 5: Class Inheritance")

class Animal {
    name: str
    age: int
    
    constructor(name: str, age: int) {
        this.name = name
        this.age = age
    }
    
    speak(): str {
        return "Some sound"
    }
    
    getInfo(): str {
        return this.name + " is " + this.age + " years old"
    }
}

class Dog extends Animal {
    breed: str
    
    constructor(name: str, age: int, breed: str) {
        super(name, age)
        this.breed = breed
    }
    
    speak(): str {
        return "Woof! Woof!"
    }
    
    getBreedInfo(): str {
        return this.name + " is a " + this.breed
    }
}

let dog: Dog = new Dog("Rex", 3, "Labrador")
print(`  ${dog.getInfo()}`)
print(`  ${dog.getBreedInfo()}`)
print(`  Sound: ${dog.speak()}`)





