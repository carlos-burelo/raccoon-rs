print("‚úÖ Test 7: Complex Methods with Logic")

class BankAccount {
    accountNumber: str
    balance: float
    isActive: bool
    
    constructor(accountNumber: str, initialBalance: float) {
        this.accountNumber = accountNumber
        this.balance = initialBalance
        this.isActive = true
    }
    
    deposit(amount: float): bool {
        if (!this.isActive) {
            return false
        }
        if (amount > 0.0) {
            this.balance = this.balance + amount
            return true
        }
        return false
    }
    
    withdraw(amount: float): bool {
        if (!this.isActive) {
            return false
        }
        if (amount > 0.0 && this.balance >= amount) {
            this.balance = this.balance - amount
            return true
        }
        return false
    }
    
    getBalance(): float {
        return this.balance
    }
    
    close(): void {
        this.isActive = false
    }
}

let account: BankAccount = new BankAccount("ACC001", 1000.0)
print(`  Account: ${account.accountNumber}`)
print(`  Initial balance: $${account.getBalance()}`)
account.deposit(500.0)
print(`  After deposit $500: $${account.getBalance()}`)
account.withdraw(200.0)
print(`  After withdraw $200: $${account.getBalance()}`)





print("‚úÖ Test 8: Nested Method Calls")

class MathOperations {
    value: int
    
    constructor(initial: int) {
        this.value = initial
    }
    
    add(n: int): MathOperations {
        this.value = this.value + n
        return this
    }
    
    multiply(n: int): MathOperations {
        this.value = this.value * n
        return this
    }
    
    getValue(): int {
        return this.value
    }
}

let math: MathOperations = new MathOperations(5)
print(`  Initial: ${math.getValue()}`)

math.add(3)
math.multiply(2)
print(`  After add(3) then multiply(2): ${math.getValue()}`)





print("‚úÖ Test 9: Class with String Operations")

class TextProcessor {
    text: str
    
    constructor(text: str) {
        this.text = text
    }
    
    toUpperCase(): str {
        return this.text.toUpper()
    }
    
    toLowerCase(): str {
        return this.text.toLower()
    }
    
    getLength(): int {
        return this.text.length
    }
    
    isEmpty(): bool {
        return this.text.isEmpty
    }
}

let processor: TextProcessor = new TextProcessor("Hello Raccoon")
print(`  Original: ${processor.text}`)
print(`  Uppercase: ${processor.toUpperCase()}`)
print(`  Lowercase: ${processor.toLowerCase()}`)
print(`  Length: ${processor.getLength()}`)
print(`  Is empty: ${processor.isEmpty()}`)





print("‚úÖ Test 10: Class with Object Composition")

class Address {
    street: str
    city: str
    zipCode: str
    
    constructor(street: str, city: str, zipCode: str) {
        this.street = street
        this.city = city
        this.zipCode = zipCode
    }
    
    getFullAddress(): str {
        return this.street + ", " + this.city + " " + this.zipCode
    }
}

class Employee {
    name: str
    id: int
    address: Address
    
    constructor(name: str, id: int, address: Address) {
        this.name = name
        this.id = id
        this.address = address
    }
    
    getInfo(): str {
        return this.name + " (ID: " + this.id + ")"
    }
    
    getFullInfo(): str {
        return this.getInfo() + " - " + this.address.getFullAddress()
    }
}

let addr: Address = new Address("123 Main St", "Springfield", "12345")
let emp: Employee = new Employee("John Doe", 1001, addr)
print(`  ${emp.getInfo()}`)
print(`  ${emp.getFullInfo()}`)





