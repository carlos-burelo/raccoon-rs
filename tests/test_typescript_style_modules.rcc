import * as M from "std:math"

fn calculateCircleArea(radius: float): float {
    return M.PI * M.pow(radius, 2.0)
}

fn calculateCircumference(radius: float): float {
    return 2.0 * M.PI * radius
}

let r = 5.0
print("âœ“ CÃ¡lculos de cÃ­rculo con radio", r)
