


fn add(x: int, y: int) {
    return x + y;
}


fn greet(name: str) {
    print("Hello, " + name);
}


fn max(a: int, b: int) {
    if (a > b) {
        return a;
    } else {
        return b;
    }
}


fn getName() {
    return "Alice";
}


fn isPositive(x: int) {
    return x > 0;
}


fn multiply(x: int, y: int): int {
    return x * y;
}


let subtract = (x: int, y: int) => x - y;


let result1 = add(5, 3);           
let result2 = max(10, 20);         
let result3 = getName();           
let result4 = isPositive(5);       
let result5 = multiply(4, 5);      
let result6 = subtract(10, 3);     

print("add(5, 3) = " + result1);
print("max(10, 20) = " + result2);
print("getName() = " + result3);
print("isPositive(5) = " + result4);
print("multiply(4, 5) = " + result5);
print("subtract(10, 3) = " + result6);
greet("Bob");
