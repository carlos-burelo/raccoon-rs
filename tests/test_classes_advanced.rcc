print("‚úÖ Test 1: Static Methods")

class MathUtils {
    value: int
    
    constructor(val: int) {
        this.value = val
    }
    
    static max(a: int, b: int): int {
        if (a > b) {
            return a
        }
        return b
    }
    
    static min(a: int, b: int): int {
        if (a < b) {
            return a
        }
        return b
    }
    
    static abs(n: int): int {
        if (n < 0) {
            return -n
        }
        return n
    }
}

print(`  max(10, 20): ${MathUtils.max(10, 20)}`)
print(`  min(10, 20): ${MathUtils.min(10, 20)}`)
print(`  abs(-15): ${MathUtils.abs(-15)}`)
print(`  abs(15): ${MathUtils.abs(15)}`)





print("‚úÖ Test 2: Getters and Setters")

class Temperature {
    celsius: float
    
    constructor(c: float) {
        this.celsius = c
    }
    
    get fahrenheit(): float {
        return (this.celsius * 9.0 / 5.0) + 32.0
    }
    
    set fahrenheit(f: float): void {
        this.celsius = (f - 32.0) * 5.0 / 9.0
    }


    get kelvin(): float {
        return this.celsius + 273.15
    }
}

let temp: Temperature = new Temperature(25.0)
print(`  Celsius: ${temp.celsius}`)
print(`  Fahrenheit: ${temp.fahrenheit}`)
print(`  Kelvin: ${temp.kelvin}`)
temp.fahrenheit = 68.0
print(`  After setting to 68¬∞F:`)
print(`  Celsius: ${temp.celsius}`)





print("‚úÖ Test 3: Method Overriding")

class Shape {
    name: str
    
    constructor(name: str) {
        this.name = name
    }
    
    area(): float {
        return 0.0
    }
    
    describe(): str {
        return "Shape: " + this.name
    }
}

class Circle extends Shape {
    radius: float
    
    constructor(radius: float) {
        super("Circle")
        this.radius = radius
    }
    
    area(): float {
        return 3.14159 * this.radius * this.radius
    }
    
    describe(): str {
        return "Circle with radius " + this.radius
    }
}

class Rectangle extends Shape {
    width: float
    height: float
    
    constructor(w: float, h: float) {
        super("Rectangle")
        this.width = w
        this.height = h
    }
    
    area(): float {
        return this.width * this.height
    }
    
    describe(): str {
        return "Rectangle " + this.width + "x" + this.height
    }
}

let circle: Circle = new Circle(5.0)
let rect: Rectangle = new Rectangle(4.0, 6.0)

print(`  ${circle.describe()}`)
print(`  Area: ${circle.area()}`)
print(`  ${rect.describe()}`)
print(`  Area: ${rect.area()}`)





print("‚úÖ Test 4: Encapsulation Pattern")

class SecureCounter {
    _count: int
    _maxValue: int
    
    constructor(max: int) {
        this._count = 0
        this._maxValue = max
    }
    
    increment(): bool {
        if (this._count < this._maxValue) {
            this._count = this._count + 1
            return true
        }
        return false
    }
    
    getCount(): int {
        return this._count
    }
    
    getMax(): int {
        return this._maxValue
    }
    
    reset(): void {
        this._count = 0
    }
}

let secCounter: SecureCounter = new SecureCounter(3)
print(`  Initial: ${secCounter.getCount()}/${secCounter.getMax()}`)
secCounter.increment()
print(`  After increment: ${secCounter.getCount()}`)
secCounter.increment()
secCounter.increment()
print(`  After 2 more increments: ${secCounter.getCount()}`)
let canIncrement: bool = secCounter.increment()
print(`  Can increment more: ${canIncrement}`)
secCounter.reset()
print(`  After reset: ${secCounter.getCount()}`)





print("‚úÖ Test 5: Complex Constructor")

class User {
    username: str
    email: str
    isActive: bool
    createdAt: str
    loginCount: int
    
    constructor(username: str, email: str) {
        this.username = username
        this.email = email
        this.isActive = true
        this.createdAt = "2024-01-01"
        this.loginCount = 0
    }
    
    login(): void {
        if (this.isActive) {
            this.loginCount = this.loginCount + 1
        }
    }
    
    deactivate(): void {
        this.isActive = false
    }
    
    getStatus(): str {
        if (this.isActive) {
            return "Active (" + this.loginCount + " logins)"
        }
        return "Inactive"
    }
}

let user: User = new User("john_doe", "john@example.com")
print(`  User: ${user.username}`)
print(`  Email: ${user.email}`)
print(`  Status: ${user.getStatus()}`)
user.login()
user.login()
user.login()
print(`  After 3 logins: ${user.getStatus()}`)
user.deactivate()
print(`  After deactivation: ${user.getStatus()}`)





print("‚úÖ Test 6: Two-Level Inheritance")

class Vehicle {
    brand: str
    
    constructor(brand: str) {
        this.brand = brand
    }
    
    info(): str {
        return "Vehicle: " + this.brand
    }
}

class Car extends Vehicle {
    doors: int
    
    constructor(brand: str, doors: int) {
        super(brand)
        this.doors = doors
    }
    
    info(): str {
        return "Car: " + this.brand + " (" + this.doors + " doors)"
    }
}





let myCar: Car = new Car("Toyota", 4)
print(`  ${myCar.info()}`)
print(`  Brand: ${myCar.brand}`)
print(`  Doors: ${myCar.doors}`)





print("‚úÖ Test 7: Complex State Management")

class ShoppingCart {
    items: str[]
    quantities: int[]
    prices: float[]
    
    constructor() {
        this.items = []
        this.quantities = []
        this.prices = []
    }
    
    addItem(name: str, quantity: int, price: float): void {
        this.items.push(name)
        this.quantities.push(quantity)
        this.prices.push(price)
    }
    
    getTotal(): float {
        let total: float = 0.0
        for (const i in 0..this.items.length - 1) {
            total = total + (this.quantities[i] * this.prices[i])
        }
        return total
    }
    
    getItemCount(): int {
        return this.items.length
    }
    
    isEmpty(): bool {
        return this.items.length == 0
    }
}

let cart: ShoppingCart = new ShoppingCart()
print(`  Cart empty: ${cart.isEmpty()}`)
cart.addItem("Apple", 3, 1.5)
cart.addItem("Banana", 5, 0.75)
cart.addItem("Orange", 2, 2.0)
print(`  Items in cart: ${cart.getItemCount()}`)
print(`  Total: $${cart.getTotal()}`)





print("‚úÖ Test 8: Methods with Multiple Returns")

class Validator {
    value: str
    
    constructor(val: str) {
        this.value = val
    }
    
    validate(): str {
        if (this.value.isEmpty) {
            return "Error: Value is empty"
        }
        if (this.value.length < 3) {
            return "Error: Value too short"
        }
        if (this.value.length > 20) {
            return "Error: Value too long"
        }
        return "Valid"
    }
}

let validator1: Validator = new Validator("")
let validator2: Validator = new Validator("ab")
let validator3: Validator = new Validator("hello")
let validator4: Validator = new Validator("this_is_a_very_long_string_that_exceeds_limit")

print(`  Empty string: ${validator1.validate()}`)
print(`  'ab': ${validator2.validate()}`)
print(`  'hello': ${validator3.validate()}`)
print(`  Long string: ${validator4.validate()}`)





