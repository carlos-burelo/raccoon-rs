print("‚úÖ Test 10: Multiple Instances Stress Test")

class Point {
    x: int
    y: int
    
    constructor(x: int, y: int) {
        this.x = x
        this.y = y
    }
    
    distance(): float {
        let xSquared: float = this.x * this.x
        let ySquared: float = this.y * this.y
        return xSquared + ySquared
    }
}

let points: Point[] = []
points.push(new Point(0, 0))
points.push(new Point(3, 4))
points.push(new Point(5, 12))
points.push(new Point(8, 15))
points.push(new Point(1, 1))

print(`  Created ${points.length} points`)
print(`  Point 0: (${points[0].x}, ${points[0].y})`)
print(`  Point 1: (${points[1].x}, ${points[1].y})`)
print(`  Point 2: (${points[2].x}, ${points[2].y})`)





