class Person {
    name: str
    age: int

    constructor(name: str, age: int) {
        print("Constructor called")
        print(`Setting name to: ${name}`)
        this.name = name
        print(`this.name is now: ${this.name}`)
        this.age = age
        print(`this.age is now: ${this.age}`)
    }
}

print("Creating person...")
let person: Person = new Person("Alice", 30)
print("Person created")
print(`person.name = ${person.name}`)
print(`person.age = ${person.age}`)
